<!DOCTYPE html>
<html>

<head>
  <% include partials/head %>
</head>

<body>
  <% include partials/menu %>

    <h1 style="text-align: center;">
      <%= page %>
    </h1>
    <h3>Personal Details</h3>
    <% profiles.forEach(function(profile){ %>
      <h4>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Name : &nbsp;&nbsp;<%=profile.name %>
      </h4>
      <h4>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Email : &nbsp;&nbsp;<%=profile.email
          %>
      </h4>
      <h4>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Mobile No : &nbsp;&nbsp;
        <%=profile.mobileno %>
      </h4>
      <h4>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Address : &nbsp;&nbsp;
        <%=profile.address %>
      </h4>
      <% }) %>
        <h3>Qualification</h3>
        <div style="padding-left: 2%;" class="row">
          <table class='table table-striped table-responsive-md btm-table'>
            <thead>

              <tr>
                <th>S.No</th>
                <th>INSTITUTE NAME</th>
                <th>PROGRAM</th>
                <th>START DATE</th>
                <th>END DATE</th>

              </tr>
            </thead>
            <tbody>
                <% educations.forEach(function(education){ %>
                  <tr>
                    <td>
                      <%=education.educationid %>
                    </td>
                    <td>
                      <%=education.inst_name %>
                    </td>
                    <td>
                      <%=education.program %>
                    </td>
                    <td>
                      <%=education.start_date %>
                    </td>
                    <td>
                      <%=education.end_date %>
                    </td>
                  </tr>
                  <% }); %>
            </tbody>
          </table>

          <h3>Work Experience</h3>
          <div style="padding-left: 2%;" class="row">
            <table style="padding-left: 25%;" class='table table-striped table-responsive-md btm-table'>
              <thead>
                <tr>&nbsp;</tr>
                <tr>
                  <th>S. No</th>
                  <th>COMPANY</th>
                  <th>Designation</th>
                  <th>START DATE</th>
                  <th>END DATE</th>
                  <th>RESPONSIBILITIES</th>
                </tr>
              </thead>
              <tbody>
                <% wexperiences.forEach(function(wexperience){ %>
                  <tr>
                    <td>
                      <%=wexperience.wexperienceid %>
                    </td>
                    <td>
                      <%=wexperience.companyname %>
                    </td>
                    <td>
                      <%=wexperience.designation %>
                    </td>
                    <td>
                      <%=wexperience.start_date %>
                    </td>
                    <td>
                      <%=wexperience.end_date %>
                    </td>
                    <td>
                      <%=wexperience.responsibilities %>
                    </td>
                  </tr>
                  <% }); %>
              </tbody>
            </table>
           </div>
           <h3>Extra Curricular Activities</h3>
           <div style="padding-left: 2%;" class="row">
            <table class='table table-striped table-responsive-md btm-table'>
                <thead>
                  <tr>&nbsp;</tr>
                    <tr>
                        <th>S. No</th>
                        <th>Activity Name</th>
                    </tr>
                </thead>
                <tbody>
                    <% activities.forEach(function(activity){ %>
                        <tr>
                            <td>
                                <%=activity.activityid %>
                            </td>
                            <td>
                                <%=activity.activityname %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
           </div>  
           <h3>Research</h3>
           <div style="padding-left: 2%;" class="row">
            <table class='table table-striped table-responsive-md btm-table'>
                <thead>

                    <tr>
                        <th>S.No</th>
                        <th>Project Name</th>
                        <th>Start Date</th>
                        <th>End Date</th>
                        <th>Research Field</th>        
                    </tr>
                </thead>
                <tbody>
                    <% rexperiences.forEach(function(rexperience){ %>
                        <tr>
                            <td>
                                <%=rexperience.rexperienceid%>
                            </td>
                            <td>
                                <%=rexperience.projectname %>
                            </td>
                            <td>
                                <%=rexperience.start_date %>
                            </td>
                            <td>
                                <%=rexperience.end_date %>
                            </td>
                            <td>
                                <%=rexperience.researchfield %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
           </div> 
           <h3>Languages</h3>
           <div style="padding-left: 2%;" class="row">
            <table class='table table-striped table-responsive-md btm-table'>
                <thead>

                    <tr>
                        <th>S.No</th>
                        <th>Language Details</th>
                    </tr>
                </thead>
                <tbody>
                    <% languages.forEach(function(language){ %>
                        <tr>
                            <td>
                                <%=language.languageid %>
                            </td>
                            <td>
                                <%=language.languagedetails %>
                            </td>
                            </tr>
                        <% }); %>
                </tbody>
            </table>
          </div>
          <h3>References</h3>
          <div style="padding-left: 2%;" class="row">
            <table class='table table-striped table-responsive-md btm-table'>
                <thead>

                    <tr>
                        <th>S.No</th>
                        <th>Name</th>
                        <th>Designation</th>
                        <th>Company</th>
                        <th>Contact No</th>

                    </tr>
                </thead>
                <tbody>
                    <% references.forEach(function(reference){ %>
                        <tr>
                            <td>
                                <%=reference.referenceid %>
                            </td>
                            <td>
                                <%=reference.name %>
                            </td>
                            <td>
                                <%=reference.designation %>
                            </td>
                            <td>
                                <%=reference.company %>
                            </td>
                            <td>
                                <%=reference.contactno %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        

        </div>






</body>
<% include partials/script %>

</html>